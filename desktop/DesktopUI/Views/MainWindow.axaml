<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:DesktopUI.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="DesktopUI.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="DesktopUI">

    <Design.DataContext>
        <vm:MainWindowViewModel/>
    </Design.DataContext>

  <StackPanel>
    <Button Command="{Binding EditLabelsCommand}">Edit Labels</Button>
    <Button Command="{Binding EditEmailsCommand}">Edit Emails</Button>
    <Button Command="{Binding EditProfilesCommand}">Edit Profiles</Button>
  </StackPanel>
</Window>
