<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <!-- Focused -->
  <SolidColorBrush x:Key="TextFocus">#0072C6</SolidColorBrush>
  <SolidColorBrush x:Key="ContentFocus">#E6F2FA</SolidColorBrush>
  <SolidColorBrush x:Key="BorderFocus">#B1D6F0</SolidColorBrush>
  <!-- Selected-->
  <SolidColorBrush x:Key="TextSelected">#0072C6</SolidColorBrush>
  <SolidColorBrush x:Key="ContentSelected">#CDE6F7</SolidColorBrush>
  <SolidColorBrush x:Key="BorderSelected">#B1D6F0</SolidColorBrush>

  <!--Combobox-->
  <SolidColorBrush x:Key="ComboBoxItemForegroundDisabled">Blue</SolidColorBrush>
  <SolidColorBrush x:Key="ComboBoxDropDownBackground">White</SolidColorBrush>
  <SolidColorBrush x:Key="ComboBoxDropDownBorderBrush">LightGray</SolidColorBrush>
  <Thickness x:Key="ComboBoxDropdownBorderThickness">1,2,1,2</Thickness>
  <CornerRadius x:Key="OverlayCornerRadius">0</CornerRadius>
</ResourceDictionary>
